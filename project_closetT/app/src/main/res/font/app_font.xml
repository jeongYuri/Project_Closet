<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute,ResourceCycle">


    <!--semi-->
    <font
        android:font="@font/leferipointwhite"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/leferipointwhite"
        app:fontStyle="normal"
        app:fontWeight="1000" />
    <!--regular-->
    <font
        android:font="@font/leferibaseregular"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/leferibaseregular"
        app:fontStyle="normal"
        app:fontWeight="500" />


    <!--bold-->
    <font
        android:font="@font/leferibasebold"
        android:fontStyle="normal"
        android:fontWeight="1000"
        app:font="@font/leferibasebold"
        app:fontStyle="normal"
        app:fontWeight="1000" />

</font-family>